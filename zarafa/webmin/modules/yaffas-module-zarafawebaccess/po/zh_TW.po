# type: string
#: lang/en:1
#, no-wrap
msgid "Option"
msgstr "選項"

# type: string
#: lang/en:2
#, no-wrap
msgid "State"
msgstr "州"

# type: string
#: lang/en:3
#, no-wrap
msgid "Save"
msgstr "儲存"

# type: string
#: lang/en:4
#, no-wrap
msgid "Options"
msgstr "選項"

# type: string
#: lang/en:5
#, no-wrap
msgid "Disable full Global Address Book"
msgstr "關閉全部全域通訊錄"

# type: string
#: lang/en:6
#, no-wrap
msgid "Enable Alphabetbar in Global Address Book"
msgstr "啟用全域通訊錄內之字母欄"

# type: string
#: lang/en:7
#, no-wrap
msgid "Enable Public Folders"
msgstr "啟用公用資料夾"

# type: string
#: lang/en:8
#, no-wrap
msgid "Disable delete in restore items"
msgstr "停用在復原項目內之刪除"

# type: string
#: lang/en:9
#, no-wrap
msgid "Enable spellchecker in HTLM editor"
msgstr "啟用HTML編輯器內之拼音檢查"

# type: string
#: lang/en:10 lang/en:13
#, no-wrap
msgid "Theme Color"
msgstr "主題顏色"

# type: string
#: lang/en:11
#, no-wrap
msgid "White"
msgstr "白色"

# type: string
#: lang/en:12
#, no-wrap
msgid "Silver"
msgstr "銀色"

# type: string
#: lang/en:15
#, no-wrap
msgid "Unknown Webaccess Option"
msgstr "不明Webaccess 選項"

# type: string
#: lang/en:16
#, no-wrap
msgid "Unknown Color"
msgstr "不明顏色"

# type: string
#: lang/en:17
#, no-wrap
msgid "Could not open webaccess config file"
msgstr "無法打開Webaccess配置檔案"

# type: string
#: lang/en:18
#, no-wrap
msgid "Please install the package \"aspell-lang\" (e.g. aspell-de for german) for spell checking in your language."
msgstr "請安裝\\\"aspell-lang\\\" (以德文拼音檢查器為例 )檢查您的語言之拼音。"

# type: string
#: lang/en:19
#, no-wrap
msgid "Webaccess"
msgstr "Webaccess"

# type: string
#: help/en:1
#, no-wrap
msgid "Specify the maximum size of attachments for each mail in MB to be uploaded via the Webaccess."
msgstr "指名各郵件經Webaccess上載之附件的最大大小(MB)"

# type: string
#: help/en:2
#, no-wrap
msgid "This limits the maximum disk usage for each user in MB."
msgstr "這限制各用戶之磁碟最大用量(MB)"

# type: string
#: help/en:3
#, no-wrap
msgid "This is the username that will be used for connecting to the database. The user has to be created and have the permissions to use the given database."
msgstr "此為用來連接資料庫之用戶名稱。用戶須被建立並擁有權限使用特定資料庫。"

# type: string
#: help/en:4
#, no-wrap
msgid "This is the password that is set on the database. You need to set this password manually."
msgstr "此為設定在資料庫上之密碼。您須用手動設定此密碼。"

# type: string
#: help/en:5
#, no-wrap
msgid "This is the address of the MySQL server. For locally installed servers enter 'localhost'."
msgstr "這是MYSQL伺服器地址，本地安裝之伺服器則請輸入「localhost」。"

# type: string
#: help/en:6
#, no-wrap
msgid "This is the name of the database to be used by Zarafa.<br />We recommend 'zarafa'.<br />It will be created if it doesn't exists. To do this the user needs the right permissions."
msgstr "這是Zarafa使用之資料庫名稱，我們建議用”Zarafa”。若不存在，將予建立。用戶須有正確權限，才能如此做。"
