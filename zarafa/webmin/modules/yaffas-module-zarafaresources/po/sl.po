msgstr "Check this box and the resource can only be booked once."

# type: string
#: help/en:4
#, no-wrap
msgid "Check this box to disable recurring appointments for this resource."
msgstr "Check this box to disable recurring appointments for this resource."

# type: string
#: help/en:5
#, no-wrap
msgid "Select the type of the resource."
msgstr "Select the type of the resource."

# type: string
#: help/en:6
#, no-wrap
