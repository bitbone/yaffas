#: lang/en:1
#, no-wrap
msgid "The IP address or netmask it not valid."
msgstr "Endereço IP ou máscara de rede invalida."

#: lang/en:2
#, no-wrap
msgid "The DNS server address has to be a valid IP address."
msgstr "O endereço do DNS precisa ser um IP válido."

#: lang/en:3 lang/en:11
#, no-wrap
msgid "The domain may only contain characters (a-z and A-Z), numbers (0-9), '.' and '-'. It also has a minimum length of two."
msgstr "O nome do domínio poderá conter somente caracteres (a-z e A-Z), números (0-9), '.' e '-', e precisa ter também um tamanho mínimo de dois."

#: lang/en:4
#, no-wrap
msgid "Could not rename domain."
msgstr "Não foi possível renomear o domínio."

#: lang/en:5
#, no-wrap
msgid "Hostname may only contain characters (a-z and A-Z), numbers (0-9) and '-'. It also has a minimum length of two."
msgstr "O nome de máquina poderá conter somente caracteres (a-z e A-Z), números (0-9) e '-', e precisa ter também um tamanho mínimo de dois."

#: lang/en:6
#, no-wrap
msgid "Workgroup may only contain characters (a-z and A-Z), numbers (0-9), '.', '_' and '-'."
msgstr "O grupo de trabalho poderá conter somente caracteres (a-z e A-Z), números (0-9), '.', '_' e '-'."

