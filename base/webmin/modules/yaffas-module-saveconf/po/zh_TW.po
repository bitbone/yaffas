#, no-wrap
msgid "please install your license-key first"
msgstr "請先安裝您的授權密鑰"

#: lang/en:10
#, no-wrap
msgid "cannot"
msgstr "無法"

#: lang/en:11
#, no-wrap
msgid "touch"
msgstr "觸碰"

#: lang/en:12
#, no-wrap
