# type: string
#: lang/en:1
#, no-wrap
msgid "Access for"
msgstr "Dostop za"

# type: string
#: lang/en:2
#, no-wrap
msgid "Community (Password)"
msgstr "Community (Password"

# type: string
#: lang/en:3
#, no-wrap
msgid "Enable SNMP"
msgstr "Enable SNMP"

# type: string
#: lang/en:4 lang/en:10
#, no-wrap
msgid "SNMP Configuration"
msgstr "SNMP Configuration"

# type: string
#: lang/en:5
#, no-wrap
msgid "Replace SNMP configuration?"
msgstr "Replace SNMP configuration?"

# type: string
#: lang/en:6
#, no-wrap
msgid "In order for yaffas to be able to perform the requested changes, it has to <strong>overwrite</strong> the current SNMP configuration.<br />Should the config be overwritten?"
msgstr "In order for yaffas to be able to perform the requested changes, it has to <strong>overwrite</strong> the current SNMP configuration.<br />Should the config be overwritten?"

# type: string
#: lang/en:8
#, no-wrap
msgid "The access can only be set for IP-Addresses or networks."
msgstr "The access can only be set for IP-Addresses or networks."

# type: string
#: lang/en:9
#, no-wrap
msgid "The community-string can only contain digits (0-9) and chars (a-z, A-Z)."
msgstr "The community-string can only contain digits (0-9) and chars (a-z, A-Z)."

# type: string
#: help/en:1
#, no-wrap
msgid "Password for the SNMP access."
msgstr "Password for the SNMP access."

# type: string
#: help/en:2
#, no-wrap
msgid "Enter a single IP address, a network address using CIDR notation (e.g. 192.168.0.0/24) or \"default\" to allow access from everywhere. Enter \"127.0.0.1\" to allow access from the local machine only."
msgstr "Enter a single IP address, a network address using CIDR notation (e.g. 192.168.0.0/24) or \"default\" to allow access from everywhere. Enter \"127.0.0.1\" to allow access from the local machine only."
