#, no-wrap
msgid "Mail aliases"
msgstr "Poštni aliasi"

#: lang/en:15
#, no-wrap
msgid "Mail alias"
msgstr "Poštni alias"

#: lang/en:16
#, no-wrap
msgid "Folder"
msgstr "Mapa"

#: lang/en:17 lang/en:20
#, no-wrap
